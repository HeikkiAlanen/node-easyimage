{
    "env": {
        "browser": false,
        "node": false,
        "amd": false
    },

    "globals": {
        "require": false,
        "exports": false,
        "process": false
    },

    "rules": {
        "no-alert": 1,
        "no-array-constructor": 0,
        "no-bitwise": 0,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-comma-dangle": 2,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-keys": 2,
        "no-else-return": 1,
        "no-empty": 2,
        "no-empty-class": 2,
        "no-empty-label": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-ex-assign": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-extra-strict": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 0,
        "no-func-assign": 2,
        "no-implied-eval": 1,
        "no-inner-declarations": [0, "functions"],
        "no-invalid-regexp": 2,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-mixed-requires": [0, false],
        "no-mixed-spaces-and-tabs": [1, false],
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-negated-in-lhs": 1,
        "no-nested-ternary": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-obj-calls": 1,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-exit": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-regex-spaces": 1,
        "no-reserved-keys": 1,
        "no-restricted-modules": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": 1,
        "no-shadow-restricted-names": 1,
        "no-space-before-semi": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 1,
        "no-sync": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 2,
        "no-underscore-dangle": 1,
        "no-unreachable": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
        "no-use-before-define": 1,
        "no-void": 1,
        "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
        "no-with": 1,
        "no-wrap-func": 1,

        "block-scoped-var": 1,
        "brace-style": [1, "1tbs"],
        "camelcase": 1,
        "complexity": [0, 11],
        "consistent-return": 1,
        "consistent-this": [1, "that"],
        "curly": [1, "all"],
        "default-case": 1,
        "dot-notation": 1,
        "eol-last": 1,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": [1, "declaration"],
        "global-strict": [1, "never"],
        "guard-for-in": 1,
        "max-depth": [0, 4],
        "max-len": [0, 81, 4],
        "max-nested-callbacks": [0, 3],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "handle-callback-err": 1,
        "new-cap": 1,
        "new-parens": 1,
        "one-var": 0,
        "quote-props": 1,
        "quotes": [0, "double"],
        "radix": 1,
        "semi": 2,
        "sort-vars": 1,
        "space-after-keywords": [1, "always"],
        "space-in-brackets": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-return-throw-case": 1,
        "space-unary-word-ops": 1,
        "strict": 1,
        "use-isnan": 1,
        "valid-jsdoc": 1,
        "valid-typeof": 1,
        "vars-on-top": 1,
        "wrap-iife": 1,
        "wrap-regex": 1,
        "yoda": [0, "never"]
    }
}
